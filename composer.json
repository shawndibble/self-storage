{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "fruitcake/laravel-cors": "3.0.0",
    "guzzlehttp/guzzle": "7.4.2",
    "inertiajs/inertia-laravel": "0.5.4",
    "laravel/framework": "9.6.0",
    "laravel/sanctum": "2.15.0",
    "laravel/tinker": "2.7.2",
    "tightenco/ziggy": "1.4.5"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "3.6.7",
    "barryvdh/laravel-ide-helper": "2.12.3",
    "spatie/laravel-ignition": "1.2.0",
    "fakerphp/faker": "1.19.0",
    "jasonmccreary/laravel-test-assertions": "2.1.1",
    "laravel/breeze": "1.9.0",
    "laravel/sail": "1.13.8",
    "mockery/mockery": "1.5.0",
    "nunomaduro/collision": "6.1.0",
    "phpunit/phpunit": "9.5.20"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}

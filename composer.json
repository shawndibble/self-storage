{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.4|^8.0",
    "fruitcake/laravel-cors": "2.0.4",
    "guzzlehttp/guzzle": "7.4.0",
    "inertiajs/inertia-laravel": "0.4.5",
    "laravel/framework": "8.70.2",
    "laravel/sanctum": "2.12.1",
    "laravel/tinker": "2.6.2",
    "tightenco/ziggy": "1.4.2"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "3.6.4",
    "barryvdh/laravel-ide-helper": "2.10.0",
    "facade/ignition": "2.16.0",
    "fakerphp/faker": "1.16.0",
    "jasonmccreary/laravel-test-assertions": "2.0.0",
    "laravel-shift/blueprint": "2.0.0",
    "laravel/breeze": "1.4.3",
    "laravel/sail": "1.12.4",
    "mockery/mockery": "1.4.4",
    "nunomaduro/collision": "5.10.0",
    "phpunit/phpunit": "9.5.10",
    "wulfheart/pretty_routes": "0.3.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
